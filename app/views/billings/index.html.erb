<p id="notice"><%= notice %></p>

<h1>Listing Billings</h1>

<table>
  <thead>
    <tr>
      <th>Tenant</th>
      <th>Total amount</th>
      <th>Due date</th>
      <th>Bill type</th>
      <th>Description</th>
      <th>Datetime paid</th>

      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @billings.each do |billing| %>
      <tr>
        <td><%= billing.tenant_id %></td>
        <td><%= billing.total_amount %></td>
        <td><%= billing.due_date %></td>
        <td><%= billing.bill_type %></td>
        <td><%= billing.description %></td>
        <td><%= billing.datetime_paid %></td>

        <td><%= link_to 'Show', billing %></td>
        <td><%= link_to 'Edit', edit_billing_path(billing) %></td>
        <td><%= link_to 'Destroy', billing, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Billing', new_billing_path %>
