<%= simple_form_for(@billing) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :tenant_id %>
    <%= f.input :total_amount %>
    <%= f.input :due_date %>
    <%= f.input :bill_type %>
    <%= f.input :description %>
    <%= f.input :datetime_paid %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
