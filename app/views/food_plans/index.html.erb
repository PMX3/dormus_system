<p id="notice"><%= notice %></p>

<h1>Listing Food Plans</h1>

<table>
  <thead>
    <tr>
      <th>Food plan date</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @food_plans.each do |food_plan| %>
      <tr>
        <td><%= food_plan.food_plan_date %></td>
        <td><%= link_to 'Show', food_plan %></td>
        <td><%= link_to 'Edit', edit_food_plan_path(food_plan) %></td>
        <td><%= link_to 'Destroy', food_plan, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Food plan', new_food_plan_path %>
