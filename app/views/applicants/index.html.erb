<p id="notice"><%= notice %></p>

<h1>Listing Applicants</h1>

<table>
  <thead>
    <tr>
      <th>Last name</th>
      <th>First name</th>
      <th>Middle initial</th>
      <th>Nickname</th>
      <th>Image path</th>
      <th>Gender</th>
      <th>Civil status</th>
      <th>Address</th>
      <th>Contact number</th>
      <th>Landline number</th>
      <th>Email</th>
      <th>Nationality</th>
      <th>Religion</th>
      <th>Birthday</th>
      <th>School</th>
      <th>School address</th>
      <th>Major</th>
      <th>Year</th>
      <th>Medical information</th>
      <th>Personal information</th>
      <th>Guardian name</th>
      <th>Guardian contact number</th>
      <th>Guardian address</th>
      <th>Guardian email</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @applicants.each do |applicant| %>
      <tr>
        <td><%= applicant.last_name %></td>
        <td><%= applicant.first_name %></td>
        <td><%= applicant.middle_initial %></td>
        <td><%= applicant.nickname %></td>
        <td><%= applicant.image_path %></td>
        <td><%= applicant.gender %></td>
        <td><%= applicant.civil_status %></td>
        <td><%= applicant.address %></td>
        <td><%= applicant.contact_number %></td>
        <td><%= applicant.landline_number %></td>
        <td><%= applicant.email %></td>
        <td><%= applicant.nationality %></td>
        <td><%= applicant.religion %></td>
        <td><%= applicant.birthday %></td>
        <td><%= applicant.school %></td>
        <td><%= applicant.school_address %></td>
        <td><%= applicant.major %></td>
        <td><%= applicant.year %></td>
        <td><%= applicant.medical_information %></td>
        <td><%= applicant.personal_information %></td>
        <td><%= applicant.guardian_name %></td>
        <td><%= applicant.guardian_contact_number %></td>
        <td><%= applicant.guardian_address %></td>
        <td><%= applicant.guardian_email %></td>
        <td><%= link_to 'Show', applicant %></td>
        <td><%= link_to 'Edit', edit_applicant_path(applicant) %></td>
        <td><%= link_to 'Destroy', applicant, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Applicant', new_applicant_path %>
