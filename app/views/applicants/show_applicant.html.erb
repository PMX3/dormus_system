<html>
<head>
  <title>DORMUS</title>
  <!--<link rel="shortcut icon" href="/assets/h-logo2.ico" />-->
  <link href='https://fonts.googleapis.com/css?family=Open+Sans:400,300,400italic,600,700' rel='stylesheet' type='text/css'>
</head>
<body>
  <div class="default_bg container-fluid">

    <div class="card">
      <div class="card_header" style="padding-left:3vw;">
      <div class="row">
        <center>
        <div class="col-md-1" style="">
          Profile
        </div>
        <div class="col-md-1" style="margin-top:-10px;">
          Statement of Account
        </div>
        <div class="col-md-1" style="margin-top:-10px;">
          My Child's Door Log
        </div>
        <div class="col-md-1" style="">
          Eligibility
        </div>
        </center>

        </div>
        
      </div>
<div class="container-fluid appli_abox">
        <div class="">
          <div class="col-md-6">
            <div id="imgholder" class="imgholder" style="background-size:cover;background-position:center;background-image:url(<%=@applicant.image_path.url%>);background-size:cover;">

            </div>
            <div class="img-base">
            </div>
          </div>
          <div class="col-md-6 appli_rbox">
      <br><br>
      <span class="sb1">Name:</span>&nbsp<%=@applicant.first_name%>
      <%=@applicant.middle_initial%>
      <%=@applicant.last_name%>
      <br><br>
      <span class="sb1">Address:</span>&nbsp<%=@applicant.address%>
      <br><br>
      <span class="sb1">Religion:</span>&nbsp<%=@applicant.religion%>
      <br><br>
      <span class="sb1">Civil Status:</span>&nbsp<%=@applicant.civil_status%>
      <br>
      <%=@applicant.nickname%>
      <br>
      <%=@applicant.gender%>
      <br>
      <%=@applicant.civil_status%>
      <br>
      <%=@applicant.contact_number%>
      <br>
      <%=@applicant.landline_number%>
      <br>
      <%=@applicant.email%>
      <br>
      <%=@applicant.nationality%>
          </div>
        </div>
      </div>



    </div>

  </div>
</body>
<!-- jQuery (necessary for Bootstrap's JavaScript plugins) -->
<script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.3/jquery.min.js"></script>
<!-- Include all compiled plugins (below), or include individual files as needed -->
<script src="bootstrap.min.js"></script>
</html>
